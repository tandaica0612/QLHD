@using (Html.BeginForm("KetQuaTimKiem", "HDNhanCong", FormMethod.Post))
                        {
                        <div class="navbar-form navbar-left" role="search">
                            <div class="form-group">
                                @Html.DropDownList("txtloaiTimKiem", (IEnumerable<SelectListItem>)ViewBag.loaiTK,new { @class = "form-control"})
                            </div>
                            <div class="form-group">
                                @Html.TextBox("txtTimKiem", "", htmlAttributes: new { @class = "form-control", @placeholder = "Nhập thông tin cần tìm ..." })
                            </div>
                          
                            <button type="submit" class="btn btn-default" style="margin-bottom:0px">Tìm kiếm</button>
                        </div>
                        }
