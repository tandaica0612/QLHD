@model QLHD.Models.THANHTOAN_CHIPHI
<div class="modal fade" id="delete" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">Thêm hợp đồng mới</h4>
                </div>     
                <div class="modal-body">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <div class="control-label col-md-4">
                                <b>Tên hợp đồng:</b>
                            </div>
                            <div class="col-md-8">
                                @Model.NGANHANG_TT
                            </div>
                        </div>
                        

                    </div>
                    <div class="clearfix"></div>
                </div>             
                <div class="modal-footer">
                    <input type="submit" class="btn btn-primary" value="Tạo mới" />
                    <input type="button" class="btn btn-default" data-dismiss="modal" value="Hủy" />
                    
                    @*<button type="button" class="btn btn-default" data-dismiss="modal">Trở lại</button>*@
                    @*<button type="button" class="btn btn-primary">OK</button>*@
                </div>
            }
        </div>
    </div>
</div>
