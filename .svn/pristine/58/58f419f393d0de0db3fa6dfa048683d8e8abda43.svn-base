@model QLHD.Models.BENTHUE_TAM
 <button type="button" data-toggle="modal" data-target="#@ViewBag.edit" title="Sửa" class="btn btn-warning btn-sm">
                <i class="fa fa-pencil" style="font-size: 15px"></i>
            </button>
<div class="modal fade" id="@ViewBag.edit" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            @using (Html.BeginForm("EditBenThuePost", "Edit", new { benthueid = @Model.BENTHUE_TAM_ID}, FormMethod.Post))
            {
                @Html.AntiForgeryToken()
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">Thêm bên thuế mới</h4>
                </div>     
                <div class="modal-body">
                    <div class="form-horizontal">
                        <div class="col-md-6">
                        <div class="form-group">
                            <div class="control-label col-md-4">
                                <b>Tên bên thuê:</b>
                            </div>
                            <div class="col-md-8">
                                <input type="text" class="form-control" name="TEN" required="required" value="@Model.TEN">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="control-label col-md-4">
                                <b>Địa chỉ:</b>
                            </div>
                            <div class="col-md-8">
                                <input type="text" class="form-control" name="DIACHI" required="required" value="@Model.DIACHI">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="control-label col-md-4">
                                <b>Điện thoại:</b>
                            </div>
                            <div class="col-md-8">
                                <input type="number" class="form-control" name="DIENTHOAI" required="required" value="@Model.DIENTHOAI">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="control-label col-md-4">
                                <b>FAX:</b>
                            </div>
                            <div class="col-md-8">
                                <input type="number" class="form-control" name="FAX" value="@Model.FAX">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="control-label col-md-4">
                                <b>Mã số thuế:</b>
                            </div>
                            <div class="col-md-8">
                                <input type="text" class="form-control" name="MS_THUE" value="@Model.MS_THUE">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="control-label col-md-4">
                                <b>Tài khoản:</b>
                            </div>
                            <div class="col-md-8">
                                <input type="text" class="form-control" name="TAIKHOAN" value="@Model.TAIKHOAN">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="control-label col-md-4">
                                <b>Ngân hàng:</b>
                            </div>
                            <div class="col-md-8">
                                <input type="text" class="form-control" name="NGANHANG" value="@Model.NGANHANG">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="control-label col-md-4">
                                <b>Đại diện:</b>
                            </div>
                            <div class="col-md-8">
                                <input type="text" class="form-control" name="DAIDIEN" value="@Model.DAIDIEN">
                            </div>
                        </div>
                            </div>
                        <div class="col-md-6">
                        <div class="form-group">
                            <div class="control-label col-md-4">
                                <b>Chức vụ:</b>
                            </div>
                            <div class="col-md-8">
                                <input type="text" class="form-control" name="CHUCVU" value="@Model.CHUCVU">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="control-label col-md-4">
                                <b>Trình độ:</b>
                            </div>
                            <div class="col-md-8">
                                <input type="text" class="form-control" name="TRINHDO" value="@Model.TRINHDO">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="control-label col-md-4">
                                <b>Công việc:</b>
                            </div>
                            <div class="col-md-8">
                                <input type="text" class="form-control" name="CONGVIEC" value="@Model.CONGVIEC">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="control-label col-md-4">
                                <b>Số CMND:</b>
                            </div>
                            <div class="col-md-8">
                                <input type="number" class="form-control" name="CMND" value="@Model.CMND">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="control-label col-md-4">
                                <b>Ngày cấp CMND:</b>
                            </div>
                            <div class="col-md-8">
                                <input type="datetimetime" class="form-control" name="NGAYCAP_CMND" value="@Model.NGAYCAP_CMND">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="control-label col-md-4">
                                <b>Nơi cấp CMND:</b>
                            </div>
                            <div class="col-md-8">
                                <input type="text" class="form-control" name="NOICAP_CMND" value="@Model.NOICAP_CMND">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="control-label col-md-4">
                                <b>Ngày sinh:</b>
                            </div>
                            <div class="col-md-8">
                                <input type="datetimetime" class="form-control" name="NGAYSINH" value="@Model.NGAYSINH">
                            </div>
                        </div>
                            </div>
                    </div>
                    <div class="clearfix"></div>
                </div>             
                <div class="modal-footer">
                    <input type="submit" class="btn btn-primary" value="Lưu" />
                    <input type="button" class="btn btn-default" data-dismiss="modal" value="Hủy" />
                </div>
            }
        </div>
    </div>
</div>